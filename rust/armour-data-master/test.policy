external logger @ "../arm-logger/log_sock" {
   fn log_request(HttpRequest) -> ()
}

fn require(req: HttpRequest) -> bool {
   logger::log_request(req);
   req.route() == ["a"]
}

fn client_payload(payload: data) -> bool {
  payload.len() < 10
}

fn server_payload(payload: data) -> bool {
  4 < payload.len()
}
